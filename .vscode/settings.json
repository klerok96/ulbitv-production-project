{
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": true,
    "source.fixAll.eslint": true
  },
  "eslint.options": {
    "overrideConfigFile": "./.eslintrc.js"
  },
  "jest.autoRun": "off",
  "jest.jestCommandLine": "npm run test:unit",
  "jest.rootPath": "src/config/jest",
  "cSpell.enableFiletypes": ["jsonc"],
  "cSpell.language": "en,ru,ru-RU",
  "i18n-ally.localesPaths": ["public/locales", "src/shared/config/i18n"],
  "i18n-ally.keystyle": "flat",
  "i18n-ally.displayLanguage": "ru",
  "i18n-ally.sourceLanguage": "ru"
}
